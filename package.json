{
  "name": "printer-report-app",
  "author": {
    "name": "Faith Yavuz"
  },
  "version": "1.0.4",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "electron:dev": "cross-env PORT=3100 NODE_ENV=development concurrently \"pnpm dev\" \"wait-on http://localhost:3100 && electron .\"",
    "electron:preview": "pnpm build && cross-env PORT=3000 NODE_ENV=production electron .",
    "electron:build:mac": "cross-env NODE_ENV=production CSC_IDENTITY_AUTO_DISCOVERY=false pnpm build && cross-env NODE_ENV=production CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac --publish never",
    "electron:build:win": "cross-env NODE_ENV=production pnpm build && cross-env NODE_ENV=production electron-builder --win"
  },
  "build": {
    "appId": "com.fatih.printerreport",
    "productName": "Printer Report",
    "directories": {
      "output": "dist/electron"
    },
    "files": [
      ".next/**",
      "public/**",
      "electron/**",
      "next.config.mjs",
      "package.json",
      "node_modules/**"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "identity": null,
      "hardenedRuntime": false,
      "icon": "public/app-icon.ico",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "public/app-icon.ico",
      "target": [
        "nsis"
      ]
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "geist": "^1.3.1",
    "input-otp": "1.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.3",
    "lucide-react": "^0.454.0",
    "next": "14.2.25",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-day-picker": "9.8.0",
    "react-dom": "^19",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.4",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.9",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^30.5.1",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5",
    "tailwindcss": "^4.1.9",
    "tw-animate-css": "1.3.3",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}
